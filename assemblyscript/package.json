{
  "name": "wasi-nn",
  "version": "0.1.0",
  "description": "WASI-NN for standalone WASM applications",
  "scripts": {
    "asbuild:untouched": "asc examples/object-classification.ts --runtime stub --use abort=examples/object-classification/wasi_abort --target debug",
    "asbuild:optimized": "asc examples/object-classification.ts --runtime stub --use abort=examples/object-classification/wasi_abort --target release",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "demo": "npm run asbuild && ./demo.sh",
    "nn": "./wasmtime run build/optimized.wasm --dir build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bytecodealliance/wasi-nn.git"
  },
  "keywords": [
    "wasi",
    "wasi-nn",
    "as"
  ],
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/bytecodealliance/wasi-nn/issues"
  },
  "homepage": "https://github.com/bytecodealliance/wasi-nn#readme",
  "dependencies": {
    "as-wasi": "^0.4.4"
  },
  "devDependencies": {
    "assemblyscript": "^0.18.9"
  },
  "directories": {
    "example": "examples"
  }
}
